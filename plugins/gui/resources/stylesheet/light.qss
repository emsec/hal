hal--PythonEditor
{
    background : qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 rgb(33, 40, 44), stop:1 rgb(23, 30, 34));

    qproperty-openIconStyle: "all->#FFFFFF";
    qproperty-openIconPath: ":/icons/folder";

    qproperty-saveIconStyle: "all->#FFFFFF";
    qproperty-saveIconPath: ":/icons/folder-down";

    qproperty-saveAsIconStyle: "all->#FFFFFF";
    qproperty-saveAsIconPath: ":/icons/save-as";

    qproperty-runIconStyle: "all->#FFFFFF";
    qproperty-runIconPath: ":/icons/run";

    qproperty-toggleMinimapIconStyle: "all->#FFFFFF";
