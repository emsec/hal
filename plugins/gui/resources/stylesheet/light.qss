*
{
    font-family                : "Iosevka";
    font-size                  : 14px;
    color                      : black;
    background                 : white;
    alternate-background-color : rgb(230, 230, 230);
    selection-background-color : rgb(180, 180, 180);
    selection-color            : white;
    show-decoration-selected   : 1;
}

*::item:selected
{
    background : rgb(200, 200, 200);
    color      : black;
}

QPushButton
{
    font           : bold "Iosevka";
    background     : white;
    border         : 1px solid black;
    padding-top    : 4px;
    padding-bottom : 4px;
    padding-left   : 14px;
    padding-right  : 14px;
    min-width      : 50px;
}

QPushButton:default
{
    background-color : rgba(180, 180, 180);
}

QPushButton:hover
{
    background-color : rgba(210, 210, 210);
}

QPushButton:checked
{
    color        : rgba(32, 36, 37, 1);
    background   : rgba(114, 140, 0, 1);
    border-color : rgba(114, 140, 0, 1);
}

QPushButton:disabled
{
    color : rgba(128, 128, 128, 1);
}

QMenuBar
{
    background-color : rgb(110, 110, 110);
    color            : rgba(228, 228, 228, 1);
}

QMenuBar::item
{
    background-color : rgb(110, 110, 110);
    color            : rgba(255, 255, 255, 1);
}

QMenu
{
    background-color : rgb(110, 110, 110);
    color            : black;
}

QDialog
{
    background-color : white;
    color            : black;
}

QLabel
{
    background-color : white;
    color            : black;
}

QLineEdit
{
    color            : black;
    font-family      : "Iosevka";
    font-style       : normal;
    background-color : white;
    border           : none;
}

QToolButton
{
    color : black;
    border           : none;
    background-color : transparent;
    font-family      : "Iosevka";
    padding          : 3px;
    margin           : 0;
}

QToolButton:hover
{
    background-color : rgba(102, 103, 105, 1);
}

QToolButton:checked
{
    color            : black;
    background-color : rgba(128, 128, 128, 0.2);
}

QToolBar
{
    background-color : rgb(100, 100, 100);
    spacing          : 1px;
    height           : 24px;
    border           : none; /* Fixes Mac Toolbar issue */
}

QToolBar QLabel
{
    font-family   : "Iosevka";
    background    : transparent;
    padding-left  : 2px;
    padding-right : 2px;
}

QToolBar QToolButton
{
    color : black;
    border           : none;
    background-color : transparent;
    font-family      : "Iosevka";
    padding          : 3px;
    margin           : 0;
}

QToolBar QToolButton:hover
{
     background-color : rgba(170, 170, 170, 0.5);
 }

QToolBar QToolButton:checked
{
    color            : #A9B7C6;
    background-color : rgba(128, 128, 128, 0.2);
}

QToolBar::separator
{
    background-color : rgba(102, 103, 105, 1);
    width            : 1;
    margin-top       : 4px;
    margin-bottom    : 4px;
}

QToolTip
{
    border        : 1px solid rgba(149, 149, 129, 1);
    border-radius : 0px;
    background    : rgba(92, 92, 72, 1);
    opacity       : 230;
    color         : rgba(216, 216, 216, 1);
    font-family   : "Iosevka";
}

QScrollBar:vertical
{
    border     : 0px;
    background : rgb(200, 200, 200);
    width      : 10px;
    margin     : 0px 0px 0px 0px;
}

QScrollBar::handle:vertical
{
    background    : qlineargradient(x1:0,
    y1            : 0,
    x2            : 1,
    y2            : 0,
    stop          : 0 #5E5E5E,
    stop          : 0.5 #575757,
    stop          : 1 #535353);
    min-height    : 25px;
    border-radius : 0px;
}

QScrollBar::add-line:vertical
{
    background          : qlineargradient(x1:0,
    y1                  : 0,
    x2                  : 1,
    y2                  : 0,
    stop                : 0 #5E5E5E,
    stop                : 0.5 #575757,
    stop                : 1 #535353);
    height              : 0px;
    subcontrol-position : bottom;
    subcontrol-origin   : margin;
}

QScrollBar::sub-line:vertical
{
    background          : qlineargradient(x1:0,
    y1                  : 0,
    x2                  : 1,
    y2                  : 0,
    stop                : 0 #5E5E5E,
    stop                : 0.5 #575757,
    stop                : 1 #535353);
    height              : 0px;
    subcontrol-position : top;
    subcontrol-origin   : margin;
}

QScrollBar:horizontal
{
    border     : 0px;
    background : rgb(200, 200, 200);
    height     : 10px;
    margin     : 0px 0px 0px 0px;
}

QScrollBar::handle:horizontal
{
    background    : qlineargradient(x1:0,
    y1            : 0,
    x2            : 1,
    y2            : 0,
    stop          : 0 #5E5E5E,
    stop          : 0.5 #575757,
    stop          : 1 #535353);
    min-width     : 25px;
    border-radius : 0px;
}

QScrollBar::add-line:horizontal
{
    background          : qlineargradient(x1:0,
    y1                  : 0,
    x2                  : 1,
    y2                  : 0,
    stop                : 0 #5E5E5E,
    stop                : 0.5 #575757,
    stop                : 1 #535353);
    width               : 0px;
    subcontrol-position : right;
    subcontrol-origin   : margin;
}

QScrollBar::sub-line:horizontal
{
    background          : qlineargradient(x1:0,
    y1                  : 0,
    x2                  : 1,
    y2                  : 0,
    stop                : 0 #5E5E5E,
    stop                : 0.5 #575757,
    stop                : 1 #535353);
    width               : 0px;
    subcontrol-position : left;
    subcontrol-origin   : margin;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical
{
    background : none;
}

QAbstractScrollArea::corner
{
    background : transparent;
    width      : 0px;
    height     : 0px;
    margin     : 0px 0px 0px 0px;
}

QListWidget
{
    background-color : rgb(14, 17, 20);
    min-width        : 200px;
    max-width        : 200px;
}

QTableWidget
{
    background-color : rgb(14, 17, 20);
